<launch>

    <param name="robot_description" command="$(find xacro)/xacro.py '$(find hratc2015_dataset)/urdf/husky.urdf.xacro'" />

    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher"/>
    <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
        <param name="rate" value="100.0" />
        <rosparam param="source_list">["/arm_controller/joint_states","/joint_states"]</rosparam>
    </node>

    <node pkg="rosbag" type="play" name="player" output="screen" args="--clock /home/coolfarm/coolfarm_workspace/src/hratc2015_dataset/bagfiles/output.bag"/>

</launch>
